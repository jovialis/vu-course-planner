{
  "name": "functions-scraping",
  "type": "module",
  "scripts": {
    "build:watch": "nodemon",
    "build": "tsc && tsc-alias"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/node": "^18.15.3",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^4.9.0"
  },
  "nodemonConfig": {
    "exec": "yarn run build",
    "ext": ".ts",
    "watch": [
      "src"
    ],
    "delay": 500
  },
  "private": true
}
